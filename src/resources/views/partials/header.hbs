{{#if notLoggin}}
    <div class="nav-container">
        <div class="wrapper">
            <div class="grid">
                <nav class="header__navbar">
                    <div class="logo">
                        <img class="logo-full"src="/assets/img/logo.svg" alt="" >
                    </div>
                    <ul class="header__navbar-list header__navbar-list-home">
                        <li class="header__navbar-item">1-University</li>
                        <li class="header__navbar-item">About</li>
                        <li class="header__navbar-item">Blog</li>
                        <li class="header__navbar-item">Contact</li>
                        <li class="header__navbar-item">Help</li>  
                    </ul>
                    <ul class="header__navbar-list">
                        <a href="/sign_in"><li class="header__navbar-item">Sign in</li></a>
                        <a href="/sign_up"><li class="header__navbar-item">Sign up</li></a>
                    </ul>    
                </nav>
            </div>
        </div>
    </div>
{{else}}
    <div id="sidebar">
        <div class="logo">
            <img src="/assets/img/logo.svg" alt="">
        </div>
        <div class="sidebar-list">
            <ul>
                <li>
                    <a href="http://localhost:3000/classroom">
                        <i class="fas fa-list"></i>
                        Classes
                    </a>
                </li>
                {{#if assignments}}
                    {{#ifEquals role "Student"}}
                        <li style="background-color: #707174;">
                            <a href="assignment?class_name={{class_name}}&classID={{classID}}"><i class="fas fa-book"></i>Assignments</a>
                        </li>
                        <li>
                            <a href="#"><i class="fas fa-user-graduate"></i>Gradebook</a>
                        </li>
                        <li>
                            <a href="#"><i class="fas fa-comment"></i>Messages</a>
                        </li>
                    {{else}}
                        <li style="background-color: #707174;">
                            <a href="assignment?class_name={{class_name}}&classID={{classID}}"><i class="fas fa-book"></i>Assignments</a>
                        </li>
                        <li>
                            <a href="./Students.html"><i class="fas fa-graduation-cap"></i>Students</a>
                        </li>
                        <li>
                            <a href="#"><i class="fas fa-poll"></i>Results</a>
                        </li>
                        <li>
                            <a href="#"><i class="fas fa-comment"></i>Messages</a>
                        </li>
                    {{/ifEquals}} 
                {{else}}
                {{/if}} 
            </ul>
        </div>
        <div class="sidebar-footer">
            <p>Need help?</p>
            <ul>
                <li>
                    <a href="#">
                        <i class="fas fa-question-circle"></i>
                        Support
                    </a>
                </li>
                <li>
                    <a href="#">
                        <i class="fas fa-envelope"></i>
                        Email us
                    </a>
                </li>
            </ul>
        </div>
    </div>
    <div id="header">
        <nav class="header__navbar">
            <a class="my-classes" href="/classroom">My Classes</a>
            {{#if assignments}}
                {{!-- <p class="arrow">></p>
                <a class="my-classes" href="classroom">{{class_name}}</a> --}}
                <p class="arrow">></p>
                <a class="my-classes-end" href="assignment?class_name={{class_name}}&classID={{classID}}">Assignments</a>
            {{else}}
            {{/if}}
            <div class="header_narbar-user">
                {{#if user.picture}}
                    <img src="{{user.picture}}" alt="" class="header__navbar-user-img">
                {{else}}
                    <img src="https://cdn2.iconfinder.com/data/icons/audio-16/96/user_avatar_profile_login_button_account_member-512.png" alt="" class="header__navbar-user-img">
                {{/if}}
                <span class="header__navbar-user-name">{{user.name}}</span>
                <ul class="header__navbar-user-menu">
                    <li class="header__navbar-user-item">
                        <a  href="/profile">My account</a>
                    </li>
                    <li class="header__navbar-user-item">
                        <a  href="/logout">Sign out</a>
                    </li>
                </ul>
            </div>
        </nav>
    </div>
{{/if}}